{
    "devCommand": "python -m uvicorn api/index:app --reload",
    "builds": [
        {
          "src": "app.py",
          "use": "@vercel/python",
          "config": {
            "maxDuration": 60,
            "pythonVersion": "3.9"
          }
        }
      ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "api/index.py"
      }
    ]
  }